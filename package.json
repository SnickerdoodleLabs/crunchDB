{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "build-maker": "tsc cli/make.ts --outDir build",
    "make": "node cli/make.js",
    "make:error-old": "node cli/make.js -m error -n",
    "make:error": "node build/cli/make.js -m error -n",
    "make:brand": "node build/cli/make.js -m brand -n",
    "make:interface": "node build/cli/make.js -m interface -n"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "crunchdb",
  "author": "Adhocmaster",
  "module": "dist/crunchdb.esm.js",
  "size-limit": [
    {
      "path": "dist/crunchdb.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/crunchdb.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.2",
    "husky": "^9.0.11",
    "size-limit": "^11.1.2",
    "ts-command-line-args": "^2.5.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "inversify": "^6.0.2",
    "neverthrow": "^6.1.0",
    "ts-brand": "^0.1.0"
  }
}
